algoritmo "semnome"
// Função :
var
num:inteiro
inicio

escreval("Digite um número: ")
leia(num)
 se num < 0 entao
 //VERDADEIRO
 escreval("NEGATIVO")
 senao
 //FALSO
 se num > 0 entao
 //VERADADEIRO
 escreval("POSITIVO")
 senao
 //FALSO
 escreval("ZERO")
 fimse
 fimse


fimalgoritmo
